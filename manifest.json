{
    "name": "emuLVC",
    "version": "0.0.1",
    "manifest_version": 2,
    "minimum_chrome_version": "23",
    "description": "emu: (L)abel (V)isualize and (C)orrect audio and derived speech audio signals",
    "icons": {
        "128": "assets/icon.png"
    },
    "app": {
    "background": {
      "scripts": ["main.js"]
    }
  },
    "offline_enabled": true,
    "permissions": ["storage",
                    "notifications",
                    "<all_urls>"
                    ]
}